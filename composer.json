{
    "name": "wellkit/yii2-stack-events",
    "description": "yii2 stack events",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "niqingyang",
            "email": "niqy@qq.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "stack\\events\\": "src/"
        }
    },
    "require": {
        "php": ">=8.1",
        "yiisoft/yii2": "^2.0",
        "yiisoft/event-dispatcher": "^1.0"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer" : true
        },
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
